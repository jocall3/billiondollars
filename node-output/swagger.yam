              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/PaymentInitiationPaymentCreateResponse"
                },
                "examples" : {
                  "example-1" : {
                    "value" : {
                      "payment_id" : "payment-id-sandbox-feca8a7a-5591-4aef-9297-f3062bb735d3",
                      "request_id" : "4ciYVmesrySiUAB",
                      "status" : "PAYMENT_STATUS_INPUT_NEEDED"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
